{
  "name": "angular-webpack",
  "version": "2.2.0",
  "description": "Skeleton project using Angular 1.6.6, typescript and webpack",
  "main": "app/index.js",
  "repository": "https://github.com/deonclem/angular-webpack-typescript.git",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.18",
    "@uirouter/core": "^5.0.19",
    "angular": "^1.7.2",
    "angular-animate": "^1.7.2",
    "angular-aria": "^1.7.2",
    "angular-i18n": "^1.7.2",
    "angular-material": "^1.1.10",
    "angular-messages": "^1.7.2"
  },
  "devDependencies": {
    "@types/angular": "^1.6.48",
    "@types/angular-material": "^1.1.59",
    "@types/angular-mocks": "^1.6.1",
    "@types/jasmine": "^2.8.8",
    "@types/webpack": "^4.4.6",
    "@types/webpack-env": "^1.13.6",
    "angular-mocks": "^1.7.2",
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "*",
    "karma-spec-reporter": "*",
    "karma-webpack": "*",
    "ng-annotate-loader": "*",
    "node-sass": "*",
    "phantomjs-prebuilt": "*",
    "postcss-loader": "*",
    "precss": "*",
    "raw-loader": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "ts-loader": "*",
    "typescript": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "scripts": {
    "test": "karma start",
    "start": "npm install && npm run-script dev",
    "dev": "node node_modules/.bin/webpack-dev-server --env.dev --content-base app --host 0.0.0.0 --no-inline",
    "build": "npm test && rm -rf dist/ && node node_modules/.bin/webpack --env.prod -p && cp app/index.html dist/index.html",
    "dist": "npm test && rm -rf dist/ && node node_modules/.bin/webpack --env.prod -p && cp app/index.html dist/index.html && node node_modules/.bin/webpack-dev-server --env.prod -p --compress --content-base dist --host 0.0.0.0 --no-inline"
  },
  "author": "deonclem",
  "license": "ISC"
}
